{
  "name": "ukg-marketplace",
  "version": "1.0.0",
  "description": "UKG Marketplace with cached CDN resources",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:backend": "node scripts/lint-backend.js",
    "lint:frontend": "node scripts/lint-frontend.js",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint:backend:fix": "node scripts/lint-backend.js --fix",
    "lint:logger": "node scripts/lint-backend.js",
    "lint:logger:fix": "node scripts/lint-backend.js --fix",
    "lint:gostyle": "node scripts/lint-backend.js",
    "lint:custom": "eslint --config config/eslint-basic-rules.js .",
    "lint:custom:fix": "eslint --config config/eslint-basic-rules.js --fix .",
    "lint:all": "node scripts/run-custom-lint.js",
    "lint:all:fix": "node scripts/run-custom-lint.js --fix",
    "setup:git-hooks": "node scripts/setup-git-hooks.js",
    "build:css": "tailwindcss -i ./src/css/input.css -o ./public/css/output.css --minify --config ./config/tailwind.config.js",
    "watch:css": "tailwindcss -i ./src/css/input.css -o ./public/css/output.css --watch --config ./config/tailwind.config.js",
    "compress:static": "node scripts/compress-static.js",
    "build": "npm run build:css && npm run compress:static",
    "dev:full": "concurrently \"cross-env NODE_ENV=development nodemon server.js\" \"npm run watch:css\"",
    "db:init": "node src/database/schema.js",
    "db:seed": "node scripts/seed-db.js",
    "db:migrate": "node scripts/migrate-db.js",
    "db:reset": "node scripts/reset-db.js",
    "db:reset-safe": "node scripts/reset-db-safe.js",
    "db:find-lock": "node scripts/find-db-lock.js",
    "db:reset-force": "node scripts/reset-db-force.js",
    "cache-cdn": "node scripts/cache-cdn.js",
    "download-material-icon-font": "node scripts/download-material-icon-font.js",
    "download-all-cdn": "npm run cache-cdn && npm run download-material-icon-font"
  },
  "keywords": [
    "UKG",
    "marketplace",
    "cdn",
    "cache"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-compression": "^1.0.2",
    "express-static-gzip": "^2.2.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "morgan-json": "^1.1.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
